<% if user_signed_in? %>
  <div class="search_form">
    <%= form_with(url: search_path,method: :get,local: true) do |f| %>
    <%= f.text_field :search %>
    <%= f.select :model , options_for_select({User: :user,Book: :book })%>
    <%= f.select :how , options_for_select({完全一致: :"0",前方一致: :"1",後方一致: :"2",部分一致: :"3" })%>
    <%= f.submit 'Search'%>
<% end %>
  </div>
<% end %>